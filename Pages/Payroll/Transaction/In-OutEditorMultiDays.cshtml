@page
@{
    Layout = "_Layout";
}


<!DOCTYPE html>


<div id="img_load" style="width:100%;text-align:center;">
    <img src="~/images/working.GIF" />
</div>
<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>In-Out Editor Multi Days <small style="color:red"><span id="lbl_ref_time"></span></small></h2>
                <div class="clearfix"></div>
                <div class="clearfix"></div>
                <div class="clearfix"></div>

                <div class="row">
                    <div class="form-horizontal">
                        <div class="col-md-12">
                            <div class="form-group">
                                <div class="form-group ">
                                    <label class="control-label col-md-2 " style="text-align:left;">
                                        Category
                                    </label>
                                    <div class="col-md-10">
                                        <input type="text" Placeholder="Enter % to Search Here .."
                                            id="txt_inoutCategory" class="form-control" />
                                    </div>
                                </div>
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Name
                                </label>
                                <div class="col-md-10">
                                    <input type="text" Placeholder="Enter % to Search Here .." id="txt_employee"
                                        class="form-control" />
                                </div>
                            </div>
                            <div class="form-group">

                                <label class="control-label col-md-2" style="text-align:left;">
                                    Date From
                                </label>
                                <div class="col-md-3">
                                    <div class='input-group date' id='txt_DateFrom'>
                                        <input type='text' class="form-control" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar">
                                            </span>
                                        </span>
                                    </div>
                                </div>
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Date To
                                </label>
                                <div class="col-md-3">
                                    <div class='input-group date' id='txt_DateTo'>
                                        <input type='text' class="form-control" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar">
                                            </span>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <button class="btn btn-light" onclick="Onload();"><i
                                            class="fa fa-filter"></i></button>
                                    <input id="btn_sav" type="button" value="Save" onclick="savrec();"
                                        class="btn btn-success" />

                                </div>
                            </div>
                        </div>

                    </div>

                </div>
                <div class="container">
                    <div class="x_content">
                        <div class="row">
                            <div class="col-md-12 col-xs-12">
                                <div class="card-box table-responsive">

                                    <!-- start project list -->
                                    <!-- @*<table id="voucher_cr_table" class="table table-striped table-bordered dataTable bs-select">
                                    *@ -->
                                    <table id="Table_View" class="table table-striped table-bordered table-sm">
                                        <thead>
                                            <tr>
                                                <th style="text-align:center;">SNo</th>
                                                <th>Date</th>
                                                <th>InnTime</th>
                                                <th>OutTime</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                    <!-- end project list -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <script src="~/FunctionJS/Payroll/Transaction/In-OutEditorMultiDays.js?@Guid.NewGuid().ToString("n")"></script>
