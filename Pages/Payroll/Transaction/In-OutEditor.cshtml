@page
@{
    Layout = "_Layout";
}


<!DOCTYPE html>


<div id="img_load" style="width:100%;text-align:center;">
    <img src="~/images/working.GIF" />
</div>
<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <div class="row">
                    <div class="col-md-3">
                        <h2>In-OutEditor</h2>
                    </div>
                    <div class="col-md-4">
                        <div class='input-group date' id='txt_Date'>
                            <input type='text' class="form-control" />
                            <span class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar">
                                </span>
                            </span>
                        </div>
                    </div>
                    <div class="col-md-1">
                        <button class="btn btn-light" onclick="Onload();"><i class="fa fa-filter"></i></button>
                    </div>
                    <div class="col-md-4">
                        <p style="text-align:right;"> <button id="btn_new" class="btn btn-info"> New</button></p>
                    </div>

                </div>
            </div>
            <div class="container">
                <div class="x_content">
                    <div class="row">
                        <div class="col-md-12 col-xs-12">
                            <div class="card-box table-responsive">

                                <!-- start project list -->
                                <!-- @*<table id="voucher_cr_table" class="table table-striped table-bordered dataTable bs-select">
                                *@ -->
                                <table id="Table_View" class="table table-striped table-bordered table-sm">
                                    <thead>
                                        <tr>
                                            <th style="text-align:center;">Action</th>
                                            <th>Date</th>
                                            <th>Employee Name</th>
                                            <th>Type</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <!-- end project list -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- @* Create Modal Start *@ -->

<div id="data_Modal" class="modal fade">
    <div class="modal-dialog modal-lg" style="width:900px !important; margin-top:6%; ">
        <div class="modal-content">
            <div class="modal-header">
                <a href="#" class="close" data-dismiss="modal">&times;</a>
                <h4>In-OutEditor </h4>
                <div id="img_load_sav" style="width:100%;text-align:center;">
                    <img src="~/images/working.GIF" />
                </div>
            </div>
            <form id="create_form">
                <div class="row">
                    <div class="form-horizontal">
                        <div class="col-md-12" style="text-align:right;">
                            <input id="btn_sav" type="button" value="Save" onclick="savrec();"
                                class="btn btn-success" />
                            <input id="btn_upd" type="button" value="Update" onclick="savrec();"
                                class="btn btn-warning" />
                            <input id="btn_cls" type="button" value="Close" onclick="discon()" class="btn btn-danger"
                                data-dismiss="modal" />
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    ID
                                </label>
                                <div class="col-md-10">
                                    <input type="text" id="txt_id" class="form-control" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Date
                                </label>
                                <div class="col-md-4">
                                    <div class='input-group date' id='txt_dat'>
                                        <input type='text' class="form-control" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar">
                                            </span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Name
                                </label>
                                <div class="col-md-10">
                                    <input type="text" Placeholder="Enter % to Search Here .." id="txt_employee"
                                        class="form-control" />
                                </div>
                            </div>
                            <div class="form-group ">
                                <label class="control-label col-md-2 " style="text-align:left;">
                                    Category
                                </label>
                                <div class="col-md-10">
                                    <input type="text" Placeholder="Enter % to Search Here .." id="txt_inoutCategory"
                                        class="form-control" />
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-2"> </div>
                                <div class="icheck-inline col-md-2">
                                    <label>
                                        <input id="ck_inn" type="checkbox" class="icheck"
                                            data-checkbox="icheckbox_square-grey">InTime
                                    </label>
                                </div>

                                <div class="col-md-3">
                                    <div class='input-group date' id='txt_inntime'>
                                        <input type='text' class="form-control" id="txtin" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar">
                                            </span>
                                        </span>
                                    </div>
                                </div>
                                <div class="icheck-inline col-md-2">
                                    <label>
                                        <input id="ck_out" type="checkbox" class="icheck"
                                            data-checkbox="icheckbox_square-grey">OutTime
                                    </label>
                                </div>
                                <div class="col-md-3">
                                    <div class='input-group date' id='txt_outtime'>
                                        <input type='text' class="form-control" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar">
                                            </span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

                <div class="modal-footer">
                </div>
            </form>
        </div>
    </div>
</div>
<!-- @* Create Modal End *@ -->

<script src="~/FunctionJS/Payroll/Transaction/In-OutEditor.js?@Guid.NewGuid().ToString("n")"></script>
