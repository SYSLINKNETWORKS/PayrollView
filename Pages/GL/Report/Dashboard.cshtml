@page
@{

    Layout = "_Layout";
}
<div class="row">
    <div class="x_panel">
        <div class="x_title">
            <div class="row">
                <div class="col-md-4">
                    <h2>ACCOUNTS DASHBOARD</h2>
                </div>
                <div class="col-md-8">
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row" style="margin-bottom: 1%;">
    @* Voucher *@
    <div class="col-md-2 col-xs-2">
        <div class="px-2" style="background-color: rgba(132, 89, 174, 0.9); color:white;padding: 2%">
            <img id="img_box_voucher" src="~/images/dashboard.GIF" />
            <p style="font-size:large;"> Voucher</p>
            <p id="box_voucher" style="font-size:large;"> 0</p>
        </div>
    </div>
    @* Bill
        <div class="col-md-2 col-xs-2">
        <div class="px-2" style="background-color:rgba(225, 71, 74, 1.0); color:white;padding: 2%">
        <img id="img_box_bill" src="~/images/dashboard.GIF" />
        <p style="font-size:large;"> Bill</p>
        <p id="box_bill" style="font-size:large;"> 0</p>
        </div>
        </div>

        Payment
        <div class="col-md-2 col-xs-2">
        <div class="px-2" style="background-color:rgba(70, 144, 188, 1.0); color:white;padding: 2%">
        <img id="img_box_payment" src="~/images/dashboard.GIF" />
        <p style="font-size:large;"> Payment</p>
        <p id="box_payment" style="font-size:large;"> 0</p>
        </div>
        </div>

        Invoice
        <div class="col-md-2 col-xs-2">
        <div class="px-2" style="background-color: rgb(77, 179, 153); color:white;padding: 2%">
        <img id="img_box_invoice" src="~/images/dashboard.GIF" />
        <p style="font-size:large;"> Invoice</p>
        <p id="box_invoice" style="font-size:large;"> 0</p>
        </div>
        </div>

        Received
        <div class="col-md-2 col-xs-2">
        <div class="px-2" style="background-color: rgba(0, 153, 76, 0.9); color:white;padding: 2%">
        <img id="img_box_received" src="~/images/dashboard.GIF" />
        <p style="font-size:large;"> Received</p>
        <p id="box_received" style="font-size:large;"> 0</p>
        </div>
        </div>

        Debit Note
        <div class="col-md-2 col-xs-2">
        <div class="px-2" style="background-color: rgba(51, 51, 153, 0.9); color:white;padding: 2%">
        <img id="img_box_debitnote" src="~/images/dashboard.GIF" />
        <p style="font-size:large;"> Debit Note</p>
        <p id="box_debitnote" style="font-size:large;"> 0</p>
        </div>
        </div> *@
</div>
<div class=" clearfix">
    <div class="clearfix"></div>
    <div class="clearfix"></div>
</div>
@* ------------------------------------------------------------- *@


    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" data-parent="#accordion">
                   Vouchers</a>
                   <a class="glyphicon glyphicon-chevron-right" style="float:right" data-toggle="collapse"
                    data-parent="#accordion" href="#collapse1" title="Expend Production" id="vouchers">
                </a>
                <span style="font-size:small;" id="spn_voucher_check" class="badge badge-pill badge-secondary">0</span>

            </h4>
        </div>
        <div id="collapse1" class="panel-collapse collapse">
            <div class="panel-body">
                  <div id="div_vouchermaster" >
                        </div>
                @* <div class="row" style="margin-bottom: 1%;">

                    @*Pending For Check/Pending For Approval Start*@
                    @* <div class="row" style="margin-bottom: 1%;">
                      
                    </div> *@

                

            </div>
        </div>
    


    @* -------------------------------------------------------------------- *@

    <!-- Modal -->
    <div class="modal fade" id="searchModelVoucher" role="dialog">
        <div class="modal-dialog" style="width:500px !important; margin-top:30%; margin-left: 60%; ">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Search</h4>
                </div>
                <div class="modal-body">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="control-label col-md-2" style="text-align:left;">
                                NO
                            </label>
                            <div class="col-md-10">
                                <input type="text" id="txt_noVoucher" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2" style="text-align:left;">
                                DateFrom
                            </label>
                            <div class="col-md-4">
                                <div class='input-group date' id='txt_datFromVoucher'>
                                    <input type='text' class="form-control" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar">
                                        </span>
                                    </span>
                                </div>
                            </div>
                            <label class="control-label col-md-2" style="text-align:left;">
                                DateTo
                            </label>
                            <div class="col-md-4">
                                <div class='input-group date' id='txt_datToVoucher'>
                                    <input type='text' class="form-control" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar">
                                        </span>
                                    </span>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" id="btnSearchVoucher" class="btn btn-default">Search</button>
                </div>
            </div>

        </div>
    </div>
    @*Received Check/Received Approval End*@

    <script src='~/FunctionJS/Notification.js?@Guid.NewGuid().ToString("n")'></script>
    <script src="~/FunctionJS/GL/Report/Dashboard/Dashboard.js?@Guid.NewGuid().ToString("n")"></script>

    <script type="module" src='~/FunctionJS/Login/DashboardPermissionByName.js?@Guid.NewGuid().ToString("n")' async
        defer></script>

    <script type="module"
        src="~/FunctionJS/GL/Report/Dashboard/MainDashboard.js?@Guid.NewGuid().ToString("n")"></script>
