@page
@{
    Layout = "_Layout";
}

<!DOCTYPE html>


<div id="img_load" style="width:100%;text-align:center;">
    <img src="~/images/working.GIF" />
</div>
<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Opening Voucher<small style="color:red"><span id="lbl_ref_time"></span></small></h2>
                <p style="text-align:right;"> <button id="btn_sav" class="btn btn-success" onclick="savrec();"> Save</button></p>
                <div class="clearfix"></div>
                <div class="clearfix"></div>
                <div class="clearfix"></div>
            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <label class="control-label col-md-2" style="text-align:left;">
                        Debit
                    </label>
                    <div class="col-md-2">
                        <label class="control-label" id="txt_dramt" style="text-align:left;">
                            0
                        </label>
                    </div>
                    <label class="control-label col-md-2" style="text-align:left;">
                        Credit
                    </label>
                    <div class="col-md-2">
                        <label class="control-label" id="txt_cramt" style="text-align:left;">
                            0
                        </label>
                    </div>
                    <label class="control-label col-md-2" style="text-align:left;">
                        Difference
                    </label>
                    <div class="col-md-2">
                        <label class="control-label" id="txt_diffamt" style="text-align:left;">
                            0
                        </label>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="x_content">
                    <div class="row">
                        <div class="col-md-12 col-xs-12">
                            <div class="card-box table-responsive" style="overflow:auto;height:500px;">

                                <!-- start project list -->
                                <table id="Table_View" class="table table-striped table-bordered table-sm">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Account</th>
                                            <th>Debit</th>
                                            <th>Credit</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <!-- end project list -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>





<script src="~/FunctionJS/GL/Setup/OpeningVoucher.js?@Guid.NewGuid().ToString("n")"></script>