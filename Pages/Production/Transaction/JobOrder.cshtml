@page
@{
    Layout = "_Layout";
}



<!DOCTYPE html>


<div id="img_load" style="width:100%;text-align:center;">
    <img src="~/images/working.GIF" />
</div>
<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Job Order<small style="color:red"><span id="lbl_ref_time"></span></small></h2>
                <p style="text-align:right;"> <button id="btn_new" class="btn btn-info"> New</button></p>
                <div class="clearfix"></div>
                <div class="clearfix"></div>
                <div class="clearfix"></div>
            </div>
            <div class="container">
                <div class="x_content">
                    <div class="row">
                        <div class="col-md-12 col-xs-12">
                            <div class="card-box table-responsive">

                                <!-- start project list -->
                                <table id="Table_View" class="table table-striped table-bordered table-sm">
                                    <thead>
                                        <tr>
                                            <th style="text-align:center;">Action</th>
                                            <th style="text-align:center;">JobCard #</th>
                                            <th style="text-align:center;">Date</th>
                                            <th style="text-align:center;">SO #</th>
                                            <th style="text-align:center;">SO Date</th>
                                            <th style="text-align:center;">Customer</th>
                                            <th style="text-align:center;">Customer PO #</th>
                                            <th style="text-align:center;">ItemPaperName</th>
                                            <th style="text-align:center;">Item</th>
                                            <th style="text-align:center;">Description</th>
                                            <th style="text-align:center;">Sheet Quantity</th>
                                            <th style="text-align:center;">Status</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <!-- end project list -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>


@* Create Modal Start *@

<div id="data_Modal" class="modal fade">
    <div class="modal-dialog modal-lg" style="width:900px !important; margin-top:6%; ">
        <div class="modal-content">
            <div class="modal-header">
                <a href="#" class="close" data-dismiss="modal">&times;</a>
                <h4>Job Order</h4>
                <div id="img_load_sav" style="width:100%;text-align:center;">
                    <img src="~/images/working.GIF" />
                </div>
            </div>
            <form id="create_form">
                <div class="row">
                    <div class="form-horizontal">

                        <div class="col-md-12" style="text-align:right;">
                            <div class="col-md-2">
                                <label class="control-label" id="txt_id" hidden>
                                </label>
                            </div>

                            <input id="btn_sav" type="button" value="Save" onclick="savrec();"
                                class="btn btn-success" />
                            <input id="btn_update" type="button" value="Update" onclick="updrec();"
                                class="btn btn-warning" />
                            <input id="btn_cls" type="button" value="Close" onclick="discon()" class="btn btn-danger"
                                data-dismiss="modal" />
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">

                                <label class="control-label col-md-2" style="text-align:left;">
                                    SO#
                                </label>
                                @* <div class="col-md-4">
                                    <input type="text" id="txt_sale" class="form-control" />
                                </div>
                                <label class="control-label col-md-1" style="text-align:left;">
                                    Item
                                </label> *@
                                <div class="col-md-10">
                                    <input type="text" id="txt_itm" class="form-control" />
                                </div>

                            </div>
                            <div class="form-group">


                                <label class="control-label col-md-2" style="text-align:left;">
                                    Description
                                </label>
                                <div class="col-md-10">

                                    <input type="text" id="txt_des" readonly class="form-control" />
                                    <input type="text" id="txt_dsoId" style="display: none;" class="form-control" />
                                </div>



                            </div>

                             <div class="form-group">


                                <label class="control-label col-md-2" style="text-align:left;">
                                    Paper Item
                                </label>
                                <div class="col-md-10">

                                    <input type="text" id="txt_pitem" class="form-control" />
                                </div>



                            </div>
                             <div class="form-group">
                              
                                <label class="control-label col-md-2" style="text-align:left;">
                                    UPS
                                </label>
                                <div class="col-md-4">
                                    <input type="text" readonly id="txt_ups" class="form-control" />

                                </div>
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Order Quantity
                                </label>
                                <div class="col-md-4">
                                    <input type="text" id="txt_orderqtypcs" class="form-control" readonly  />
                                </div>
                            </div>
                            
                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Delivery Date
                                </label>
                                <div class="col-md-3">
                                    <input type="text" id="txt_ddate" readonly class="form-control" />
                                </div>
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Sheet Qty
                                </label>
                                <div class="col-md-2">
                                    <input type="text" readonly id="txt_orderqtysheet" onclick="Get_Des()"
                                        class="form-control" />

                                </div>
                                <label class="control-label col-md-1" style="text-align:left;">
                                  Sheet Qty
                                </label>
                                <div class="col-md-2">
                                    <input type="text" id="txt_qty" class="form-control" value="0" />
                                </div>
                            </div>

                            

                        </div>
                    </div>


                </div>

                <div class="modal-footer">
                </div>
            </form>
        </div>
    </div>
</div>
@* Create Modal End *@


<script src="~/FunctionJS/Production/Transaction/JobOrder.js?@Guid.NewGuid().ToString("n")"></script>