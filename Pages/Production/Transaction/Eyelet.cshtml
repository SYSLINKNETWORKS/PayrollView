@page
@{
    Layout = "_Layout";
}
<!DOCTYPE html>

<div id="img_load" style="width:100%;text-align:center;">
    <img src="~/images/working.GIF" />
</div>
<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Eyelet Department<small style="color:red"><span id="lbl_ref_time"></span></small></h2>
                <p style="text-align:right;"> <button id="btn_new" class="btn btn-info"> New</button></p>
                <div class="clearfix"></div>
            </div>
            <div class="container">
                <div class="x_content">
                    <div class="row">
                        <div class="col-md-12 col-xs-12">
                            <div class="card-box table-responsive">
                                <!-- start project list -->
                                <table id="Table_View" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th style="text-align:center;">Action</th>
                                            <th style="text-align:center;">Date</th>
                                            <th style="text-align:center;">JobCard #</th>
                                            <th style="text-align:center">Item</th>
                                            <th style="text-align:center;">Description</th>
                                            <th style="text-align:center">Production</th>

                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <!-- end project list -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>


@* Create Modal Start *@

<div id="data_Modal" class="modal fade">
    <div class="modal-dialog modal-lg" style="width:900px !important; margin-top:6%; ">
        <div class="modal-content">
            <div class="modal-header">
                <a href="#" class="close" data-dismiss="modal">&times;</a>
                <h4>Eyelet</h4>
                <div id="img_load_sav" style="width:100%;text-align:center;">
                    <img src="~/images/working.GIF" />
                </div>
            </div>
            <form id="create_form">
                <div class="row">
                    <div class="form-horizontal">
                        <div class="col-md-12" style="text-align:right;">
                            <input id="btn_sav" type="button" value="Save" onclick="savrec();"
                                class="btn btn-success" />
                            <input id="btn_update" type="button" value="Update" onclick="updrec();"
                                class="btn btn-warning" />
                            <input id="btn_cls" type="button" value="Close" onclick="discon()" class="btn btn-danger"
                                data-dismiss="modal" />
                        </div>
                        <div class="col-md-12">
                            <label class="control-label" id="txt_id" hidden> </label>

                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Date
                                </label>
                                <div class="col-md-4">
                                    <div class='input-group date' id='txt_dat'>
                                        <input type='text' class="form-control" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar">
                                            </span>
                                        </span>
                                    </div>
                                </div>


                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Job #
                                </label>
                                <div class="col-md-10">
                                    <input type="text" id="txt_job" class="form-control" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Item
                                </label>
                                <div class="col-md-10">
                                    <input type="text" readonly id="txt_itm" class="form-control" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    D.Date
                                </label>
                                <div class="col-md-4">
                                    <input type="text" id="txt_ddat" readonly class="form-control" />
                                </div>
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Qty
                                </label>
                                <div class="col-md-4">
                                    <input type="text" id="txt_qty" readonly class="form-control" />
                                </div>
                            </div>


                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Time
                                </label>
                                <div class="col-md-4">
                                    @* <input type="text" id="txt_time" class="form-control" /> *@
                                    <select id="ddl_time" class="form-control">
                                        <option value="01:00">01:00</option>
                                        <option value="02:00">02:00</option>
                                        <option value="03:00">03:00</option>
                                        <option value="04:00">04:00</option>
                                        <option value="05:00">05:00</option>
                                        <option value="06:00">06:00</option>
                                        <option value="07:00">07:00</option>
                                        <option value="08:00">08:00</option>
                                        <option value="09:00">09:00</option>
                                        <option value="10:00">10:00</option>
                                        <option value="11:00">11:00</option>
                                        <option value="12:00">12:00</option>
                                        <option value="13:00">13:00</option>
                                        <option value="14:00">14:00</option>
                                        <option value="15:00">15:00</option>
                                        <option value="16:00">16:00</option>
                                        <option value="17:00">17:00</option>
                                        <option value="18:00">18:00</option>
                                        <option value="19:00">19:00</option>
                                        <option value="20:00">20:00</option>
                                        <option value="21:00">21:00</option>
                                        <option value="22:00">22:00</option>
                                        <option value="23:00">23:00</option>
                                        <option value="00:00">00:00</option>
                                    </select>
                                </div>
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Machine
                                </label>
                                <div class="col-md-4">
                                    <input type="text" id="txt_machine" class="form-control" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Production Qty
                                </label>
                                <div class="col-md-2">
                                    <input type="text" id="txt_productionqty" value="0" class="form-control" />
                                </div>
                                <label class="control-label col-md-2" style="text-align:left;">
                                    Wastage
                                </label>
                                <div class="col-md-2">
                                    <input type="text" id="txt_wastageqty" value="0" class="form-control" />
                                </div>
                                <div class="col-md-2">

                                    <input id="btn_cls" type="button" value="Add" onclick="AddDetail()"
                                        class="btn btn-success" />
                                </div>
                            </div>

                            <div class="form-horizontal">
                                <div class="col-md-12">
                                    <table id="detailsTable" class="table table-striped table-bordered">
                                        <thead style="text-align:center">
                                            <tr>
                                                <th style="text-align:center;">Action</th>
                                                <th style="text-align:center;">Time</th>
                                                <th style="text-align:center;" hidden>Machine Id</th>
                                                <th style="text-align:center;">Machine #</th>
                                                <th style="text-align:center;">ProductionQty</th>
                                                <th style="text-align:center;">Wastage Qty</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="modal-footer">
                </div>
            </form>
        </div>
    </div>
</div>
@* Create Modal End *@



<script src="~/FunctionJS/Production/Transaction/Eyelet.js?@Guid.NewGuid().ToString("n")"></script>