@page
@{
    Layout = "_Layout";
}
<!DOCTYPE html>
<div id="img_load" style="width:100%;text-align:center;">
    <img src="~/images/working.GIF" />
</div>
<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Branch<small style="color:red"><span id="lbl_ref_time"></span></small></h2>
                <p style="text-align:right;">
                    <button id="btn_new" class="btn btn-info"> New</button>
                </p>
                <div class="clearfix"></div>
            </div>
            <div class="container">
                <div class="x_content">
                    <div class="row">
                        <div class="col-md-12 col-xs-12">
                            <div class="card-box table-responsive">

                                <!-- start project list -->
                                <table id="Table_View" class="table table-striped table-bordered table-sm">
                                    <thead>
                                        <tr>
                                            <th style="text-align:center;">Action</th>
                                            <th>Company Name</th>
                                            <th>Branch Name</th>
                                            <th>Address</th>
                                            <th>Phone</th>

                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <!-- end project list -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

@* Create Modal Start *@
<div id="data_Modal" class="modal fade">
    <div class="modal-dialog modal-lg" style="width:900px !important;">
        <div class="modal-content">
            <div class="modal-header">
                <a href="#" class="close" data-dismiss="modal">&times;</a>
                <h4>Branch</h4>
                <div id="img_load_sav" style="width:100%;text-align:center;">
                    <img src="~/images/working.GIF" />
                </div>
            </div>
            <form id="create_form">
                @*Master*@
                <div class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-12" style="text-align:right;">
                            <input id="btn_sav" type="button" value="Save" onclick="savrec()" class="btn btn-success" />
                            <input id="btn_update" type="button" value="Update" onclick="updrec()"
                                class="btn btn-warning" />
                            <input id="btn_cls" type="button" value="Close" onclick="discon()" class="btn btn-danger"
                                data-dismiss="modal" />
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-2" id="txt_id" style="display:none;">
                        </div>

                        <label class="control-label col-md-2" style="text-align:left;">
                            Name
                        </label>
                        <div class="col-md-4">
                            <input type="text" id="txt_nam" class="form-control" placeholder="Name" />
                        </div>
                        <label class="control-label col-md-2" style="text-align:left;">
                            Short Name
                        </label>
                        <div class="col-md-4">
                            <input type="text" id="txt_shortName" class="form-control" placeholder="Short Name"
                                maxlength="10" />
                        </div>



                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-2" style="text-align:left;">
                            Company
                        </label>
                        <div class="col-md-10">
                            <input type="text" id="txt_com" class="form-control" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-2" style="text-align:left;">
                            Address
                        </label>
                        <div class="col-md-4">
                            <textarea id="txt_add" rows="5" class="form-control" placeholder="Address"></textarea>
                        </div>

                        <label class="control-label col-md-2" style="text-align:left;">
                            Phone
                        </label>
                        <div class="col-md-4">
                            <input type="text" id="txt_pho" class="form-control" placeholder="Phone #" />
                        </div>
                        <label class="control-label col-md-2" style="text-align:left;">
                            Mobile
                        </label>

                        <div class="col-md-4">
                            <input type="text" id="txt_mob" class="form-control" placeholder="Mobile #" />
                        </div>
                        <label class="control-label col-md-2" style="text-align:left;">
                            Email
                        </label>
                        <div class="col-md-4">
                            <input type="text" id="txt_email" placeholder="Email Address" class="form-control" />
                        </div>
                        <label class="control-label col-md-2" style="text-align:left;">
                            Web
                        </label>
                        <div class="col-md-4">
                            <input type="text" id="txt_web" placeholder="Web Address" class="form-control" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-2" style="text-align:left;">
                            Fax
                        </label>
                        <div class="col-md-4">
                            <input type="text" id="txt_fax" placeholder="Fax" class="form-control" />
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                </div>
            </form>
        </div>
    </div>
</div>
@* Create Modal End *@
<script src="~/FunctionJS/Configuration/Setup/Branch.js?@Guid.NewGuid().ToString("n")"></script>