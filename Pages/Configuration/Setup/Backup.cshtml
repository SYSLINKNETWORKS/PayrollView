@page
@{
    Layout = "_Layout";
}

<div id="img_load" style="width:100%;text-align:center;">
    <img src="~/images/working.GIF" />
</div>

<div class="x_panel">
    <div class="x_title">
        <h2>BACKUP<small style="color:red"><span id="lbl_ref_time"></span></small></h2>
          
        <p style="text-align:right;">
            <input type="text" id="txt_service" style="width:20%;text-align:left;"/>
            <input id="btn_show" type="button" value="Show Backup" onclick="show_backup()" class="btn btn-primary" style="width:11%; text-align:right;" />
            <input id="btn_backup" type="button" value="Create Backup" onclick="create_backup()" class="btn btn-primary" style="width:11%;text-align:right;" />
        </p>
        <div class="clearfix"></div>
    </div>
    <div class="container">
        <div class="x_content">


            <div class="row">
                <div class="col-md-12 col-xs-12">
                    <div class="card-box table-responsive">
                        <!-- start project list -->
                        <table id="Table_View" class="table table-striped table-bordered table-sm">
                            <thead>
                                <tr>
                                    <th style="text-align:center;">Action</th>
                                    <th>S#</th>
                                    <th>Date</th>
                                    <th>Time</th>
                                    <th>File Name</th>
                                    <th>Size</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                        <!-- end project list -->
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<script src="~/FunctionJS/Configuration/Setup/BACKUP.js?@Guid.NewGuid().ToString("n")"></script>