@page
@{
    Layout = "_Layout";
}

<style>
    .Error {
        color: Red;
    }

    .Ok {
        color: Green;
    }
</style>


<div class="x_panel">
    <div class="x_title">
        <h2>Service Health Check<small style="color:red"><span id="lbl_ref_time"></span></small></h2>

        <p style="text-align:right;">
        </p>
        <div class="clearfix"></div>
    </div>
    <div class="container">
        <div class="x_content">

            <div id="img_load" style="width:100%;text-align:center;">
                <img src="~/images/working.GIF" />
            </div>
            <div class="row">
                <div class="col-md-12 col-xs-12">
                    <div class="card-box table-responsive">
                        <table id="Table_View" class="table table-striped table-bordered table-sm">
                            <thead>
                                <tr>
                                    <th>S#</th>
                                    <th>Service Name</th>
                                    <th>Version</th>
                                    <th>Health</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                        <!-- end project list -->
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<script src="~/FunctionJS/HealthCheck.js?@Guid.NewGuid().ToString("n")"></script>